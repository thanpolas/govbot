{
    "name": "govbot",
    "version": "0.0.1",
    "main": "./app/",
    "description": "Informs and populates information regarding uniswap governance.",
    "database_name": "govbot",
    "homepage": "https://github.com/thanpolas/govbot",
    "bugs": "https://github.com/thanpolas/govbot/issues",
    "author": {
        "name": "Thanos Polychronakis",
        "email": "thanpolas@gmail.com"
    },
    "contributors": [],
    "repository": {
        "type": "git",
        "url": "https://github.com/thanpolas/govbot"
    },
    "license": "ISC",
    "engines": {
        "node": "^16.11.0"
    },
    "scripts": {
        "start": "node .",
        "eslint": "eslint app test",
        "db:reset:local": "node scripts/db-local.script.js",
        "knex:create_migration": "knex --knexfile ./migrations/knexfile.js --migrations-directory ./migrations/migrations/ migrate:make",
        "jest": "TZ=utc jest --ci --coverage && codecov",
        "knex:migrate": "knex --knexfile ./migrations/knexfile.js --migrations-directory ./migrations/migrations/ migrate:latest",
        "test": "npm run eslint && npm run jest"
    },
    "jest": {
        "coverageDirectory": "./coverage/",
        "collectCoverage": true,
        "collectCoverageFrom": [
            "./app/**/*.js"
        ],
        "coverageReporters": [
            "json",
            "html"
        ],
        "roots": [
            "./test/app",
            "./test/unit"
        ],
        "testEnvironment": "node",
        "setupFilesAfterEnv": [
            "jest-extended"
        ],
        "globalSetup": "./test/lib/global-setup.test.js",
        "testTimeout": 10000
    },
    "dependencies": {
        "axios": "0.24.0",
        "bluebird": "3.7.2",
        "body-parser": "1.19.0",
        "config": "3.3.6",
        "connection-string": "4.3.4",
        "cookie-parser": "1.4.5",
        "date-fns": "2.25.0",
        "discord.js": "13.3.0",
        "dont-sniff-mimetype": "1.1.0",
        "dotenv": "10.0.0",
        "express": "4.17.1",
        "express-sslify": "1.2.0",
        "http-shutdown": "1.2.2",
        "invariant": "2.2.4",
        "knex": "0.95.12",
        "lodash": "4.17.21",
        "logality": "3.1.1",
        "nocache": "3.0.1",
        "node-cron": "3.0.0",
        "pg": "8.7.1",
        "truncate": "3.0.0",
        "twitter": "1.7.1",
        "uuid": "8.3.2",
        "validator": "13.6.0"
    },
    "devDependencies": {
        "@types/jest": "27.0.2",
        "codecov": "3.8.3",
        "eslint": "8.1.0",
        "eslint-config-airbnb-base": "14.2.1",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "2.25.2",
        "eslint-plugin-jest": "25.2.2",
        "eslint-plugin-jsdoc": "37.0.3",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-security-node": "^1.0.14",
        "expect": "27.3.1",
        "faker": "5.5.3",
        "jest": "27.3.1",
        "jest-extended": "1.1.0",
        "jest-junit": "13.0.0",
        "prettier": "2.4.1",
        "supertest": "6.1.6"
    }
}
